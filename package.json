{
    "name": "MyApp",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "yarn-clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean --force && npm cache verify && yarn --clean -cache",
