        "@types/jest": "^25.2.3",
        "@types/react-native": "^0.63.46",
        "@types/react-native-material-ripple": "^0.9.2",
        "@types/react-test-renderer": "^16.9.2",
        "babel-jest": "^25.1.0",
        "eslint": "^6.8.0",
        "eslint-plugin-react-hooks": "^3.0.0",
        "jest": "^25.1.0",
        "metro-react-native-babel-preset": "^0.59.0",
